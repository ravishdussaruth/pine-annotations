{
    "name": "larachimp/pine-annotations",
    "description": "Easing annotations reading in Laravel",
    "keywords": ["php", "Laravel", "annotations", "reader"],
    "license": "MIT",
    "authors": [
        {
            "name": "Percy Mamedy",
            "email": "percymamedy@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "doctrine/annotations": "^2.0",
        "doctrine/cache": "^2.1",
        "illuminate/support": "^8.0|^9.0",
        "symfony/finder": "^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "LaraChimp\\PineAnnotations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaraChimp\\PineAnnotations\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "LaraChimp\\PineAnnotations\\PineAnnotationsServiceProvider"
            ],
            "aliases": {
                "AnnotationsReader": "LaraChimp\\PineAnnotations\\Facades\\Reader"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
